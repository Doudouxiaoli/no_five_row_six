<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="V2.0.0" author="dxl">

        <comment>演唱会表添加场馆,歌曲段</comment>
        <addColumn tableName="frs_concert">
            <column name="fc_venue" type="varchar(128)" remarks="场馆"/>
            <column name="fc_song_type" type="int(11)" remarks="音源类型 0:现场 1:线上"/>
            <column name="fc_songs" type="varchar(128)" remarks="歌曲名称"/>
        </addColumn>

    </changeSet>
</databaseChangeLog>